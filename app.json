{
  "expo": {
    "name": "REV Delivery",
    "slug": "shopify-react-native-template",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sfkunal.shopifyreactnativetemplate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "updates": {
      "url": "https://u.expo.dev/d36086a4-c412-4717-904e-3841f6a42d78"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d36086a4-c412-4717-904e-3841f6a42d78"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}